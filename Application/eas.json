{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "file:GOOGLE_SERVICES_JSON",
        "EXPO_PUBLIC_SERVER_URL": "REPLACE_ME_IN_EAS_SECRETS",
        "EXPO_PUBLIC_THIRDWEB_CLIENT_ID": "REPLACE_ME_IN_EAS_SECRETS",
        "EXPO_PUBLIC_THIRDWEB_SECRET_KEY": "REPLACE_ME_IN_EAS_SECRETS"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": "file:GOOGLE_SERVICES_JSON"
      }
    }
  },
  "submit": {
    "production": {}
  }
}