{
  "expo": {
    "name": "chamapay-app",
    "slug": "chamapay-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chamapay-logo.png",
    "scheme": "chamapayapp",
    "userInterfaceStyle": "light",
    "backgroundColor": "#d1f6f1",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/chamapay-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#d1f6f1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeff.chamapay",
      "usesAppleSignIn": true,
      "backgroundColor": "#d1f6f1",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Use Face ID to securely access your wallet seed phrase",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "backgroundColor": "#d1f6f1",
      "windowSoftInputMode": "adjustResize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chamapay-logo.png",
        "backgroundColor": "#d1f6f1"
      },
      "splash": {
        "image": "./assets/images/chamapay-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#d1f6f1",
        "mdpi": "./assets/images/chamapay-logo.png",
        "hdpi": "./assets/images/chamapay-logo.png",
        "xhdpi": "./assets/images/chamapay-logo.png",
        "xxhdpi": "./assets/images/chamapay-logo.png",
        "xxxhdpi": "./assets/images/chamapay-logo.png"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.jeff.chamapay",
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "softwareKeyboardLayoutMode": "resize",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.chamapay.app",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "chamapayapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.jeff.chamapay"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/chamapay-logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#d1f6f1"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Use Face ID to securely access your wallet seed phrase"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication",
      "expo-web-browser",
      "expo-notifications",
      "@react-native-community/datetimepicker",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f20febb9-ea93-4de4-8e4d-ad1e6c5808d9"
      }
    }
  }
}